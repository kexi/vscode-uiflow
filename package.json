{
  "name": "uiflow",
  "displayName": "UiFlow",
  "description": "UiFlow support For Visual Studio Code.",
  "author": "Kei Nakayama",
  "license": "MIT",
  "version": "0.1.0",
  "publisher": "kexi",
  "galleryBanner": {
    "color": "#79b74a",
    "theme": "dark"
  },
  "icon": "icon.png",
  "engines": {
    "vscode": "^0.10.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kexi/vscode-uiflow.git"
  },
  "bugs": {
    "url": "https://github.com/kexi/vscode-uiflow/issues"
  },
  "categories": [
    "Languages",
    "Other",
    "Snippets"
  ],
  "homepage": "https://github.com/kexi/vscode-uiflow/blob/master/README.md",
  "activationEvents": [
    "onLanguage:uiflow",
    "onCommand:uiflow.openPreview",
    "onCommand:uiflow.exportSVG",
    "onCommand:uiflow.openExport"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "uiflow.openPreview",
        "title": "Open Preview to the Side",
        "category": "UiFlow"
      },
      {
        "command": "uiflow.openSource",
        "title": "Open Source",
        "category": "UiFlow"
      },
      {
        "command": "uiflow.exportSVG",
        "title": "Export SVG",
        "category": "UiFlow"
      },
      {
        "command": "uiflow.openExport",
        "title": "Export PNG",
        "category": "UiFlow"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "resourceLangId == uiflow",
          "command": "uiflow.openPreview",
          "group": "navigation"
        },
        {
          "when": "resourceScheme == uiflow",
          "command": "uiflow.openSource",
          "group": "navigation"
        },
        {
          "when": "resourceLangId == 'uiflow'",
          "command": "uiflow.exportSVG",
          "group": "export"
        },
        {
          "when": "resourceLangId == uiflow",
          "command": "uiflow.openExport",
          "group": "export"
        }
      ],
      "editor/context": [
        {
          "when": "resourceScheme == uiflow",
          "command": "uiflow.openSource",
          "group": "navigation"
        },
        {
          "when": "resourceLangId == 'uiflow'",
          "command": "uiflow.exportSVG",
          "group": "export"
        },
        {
          "when": "resourceLangId == uiflow",
          "command": "uiflow.openExport",
          "group": "export"
        }
      ],
      "explorer/context": [
        {
          "when": "resourceLangId == uiflow",
          "command": "uiflow.openPreview",
          "group": "navigation"
        },
        {
          "when": "resourceLangId == 'uiflow'",
          "command": "uiflow.exportSVG",
          "group": "export"
        },
        {
          "when": "resourceLangId == uiflow",
          "command": "uiflow.openExport",
          "group": "export"
        }
      ]
    },
    "snippets": [
      {
        "language": "uiflow",
        "path": "./snippets/uiflow.json"
      }
    ],
    "languages": [
      {
        "id": "uiflow",
        "extensions": [
          ".uif",
          ".uif.md",
          ".uif.text",
          ".uif.txt"
        ],
        "aliases": [
          "UiFlow"
        ]
      }
    ],
    "keybindings": [
      {
        "command": "uiflow.openPreview",
        "key": "alt+u p"
      },
      {
        "command": "uiflow.exportSVG",
        "key": "alt+u s"
      },
      {
        "command": "uiflow.openExport",
        "key": "alt+u n"
      },
      {
        "command": "uiflow.openExport",
        "key": "alt+u e"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "watch": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "compile": "node ./node_modules/vscode/bin/compile -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "lint": "node ./node_modules/tslint/bin/tslint ./src/*.ts ./test/*.ts",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "bower": "^1.7.9",
    "tslint": "^3.3.0",
    "typescript": "^1.8.9",
    "vscode": "^0.11.0"
  },
  "dependencies": {
    "fs-extra": "^0.30.0",
    "jquery": "^3.1.0",
    "parsimmon": "^0.7.1",
    "through2": "^2.0.1",
    "tmp": "0.0.28",
    "uiflow": "1.1.8"
  }
}
